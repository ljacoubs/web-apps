<h1>Contacts</h1>


<% contacts = Contact.all%>

<p>There are <%= contacts.count %> contacts</p>

<ul>
<% for contact in contacts%>
<li> 

<p> <%=contact["first_name"]%> 
<%=contact["last_name"]%> <p>
<p><%=contact["email"]%><p>
<%=contact["phone_number"]%>
<%=contact["company_id"]%>

<%= company = Company.find_by({"id" => contact["company_id"]})%>
</li>
<%end%>

</ul>